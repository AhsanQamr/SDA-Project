<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectui.cancelController">
   <children>
      <Label alignment="CENTER" layoutX="15.0" layoutY="51.0" prefHeight="31.0" prefWidth="574.0" text="Please provide the following information">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
      </Label>
      <Label layoutX="115.0" layoutY="134.0" text="Full Name">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="115.0" layoutY="180.0" text="Ticket No.">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="fullnamelabel" layoutX="236.0" layoutY="135.0" prefHeight="25.0" prefWidth="171.0" />
      <TextField fx:id="ticketnolabel" layoutX="236.0" layoutY="181.0" prefHeight="25.0" prefWidth="171.0" />
      <Button fx:id="submitbuttoncancel" layoutX="228.0" layoutY="268.0" mnemonicParsing="false" onAction="#setSubmitButtonCancel" text="Submit">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="cancelticketbutton" layoutX="335.0" layoutY="268.0" mnemonicParsing="false" onAction="#setcancelticketbutton" text="Cancel">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="cancelbackbutton" layoutX="34.0" layoutY="349.0" mnemonicParsing="false" onAction="#setcancelbackutton" text="Back" />
      <Label fx:id="promptatcancel" alignment="CENTER" layoutX="149.0" layoutY="107.0" prefHeight="17.0" prefWidth="252.0" text="*">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
