<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectui.FareContoller">
   <children>
      <Label alignment="CENTER" layoutX="72.0" layoutY="36.0" prefHeight="54.0" prefWidth="456.0" text="Fare Enquiry for Trains between Stations">
         <font>
            <Font name="Britannic Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="38.0" layoutY="135.0" prefHeight="32.0" prefWidth="160.0" text="Depart. Station">
         <font>
            <Font name="AdobeDevanagari-Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="38.0" layoutY="200.0" prefHeight="32.0" prefWidth="160.0" text="Arrival Station">
         <font>
            <Font name="AdobeDevanagari-Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="calculatebutton" layoutX="263.0" layoutY="285.0" mnemonicParsing="false" onAction="#setCalculatebutton" prefHeight="46.0" prefWidth="160.0" text="Calculate">
         <font>
            <Font name="Algerian" size="20.0" />
         </font>
      </Button>
      <Button fx:id="backbutton" layoutX="20.0" layoutY="361.0" mnemonicParsing="false" onAction="#setBackbutton" text="Back" />
      <ChoiceBox fx:id="departchoicebox" layoutX="273.0" layoutY="139.0" prefHeight="25.0" prefWidth="160.0" />
      <ChoiceBox fx:id="arrivalchoicebox" layoutX="273.0" layoutY="204.0" prefHeight="25.0" prefWidth="160.0" />
      <Label fx:id="mylabel" layoutX="500.0" layoutY="183.0" text="Label" />
      <Label fx:id="promptatfare" alignment="CENTER" layoutX="108.0" layoutY="105.0" prefHeight="17.0" prefWidth="302.0" text="*">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
