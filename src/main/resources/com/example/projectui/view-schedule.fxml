      <Label layoutX="56.0" layoutY="152.0" prefHeight="32.0" prefWidth="143.0" text="Depart. Station">
         <font>
            <Font name="AdobeDevanagari-Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="56.0" layoutY="236.0" prefHeight="32.0" prefWidth="143.0" text="Arrival Station">
